<?xml version="1.0"?>
<extension key="config-profiles" type="module">
  <file>config_profiles</file>
  <name>Configuration Profiles</name>
  <description>This extension provides a data structure for configuration that
    is organized in configurable profiles
  </description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA GmbH</author>
    <email>info@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/config-profiles</url>
    <url desc="Documentation">https://github.com/systopia/config-profiles</url>
    <url desc="Support">https://github.com/systopia/config-profiles</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-05-10</releaseDate>
  <version>1.0-alpha6</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.58</ver>
  </compatibility>
  <comments>This extension is most likely only needed to be installed as a
    dependency of other extensions.
  </comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/ConfigProfiles</namespace>
    <format>23.02.1</format>
    <angularModule>crmConfigProfiles</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_ConfigProfiles_Upgrader</upgrader>
</extension>
